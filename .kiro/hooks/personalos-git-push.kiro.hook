{
  "enabled": true,
  "name": "PersonalOS Git Push",
  "description": "Commit and push Tasks to git",
  "version": "1",
  "when": {
    "type": "userTriggered"
  },
  "then": {
    "type": "askAgent",
    "prompt": "Commit and push tasks to git:\n\n1. Run: git add Tasks/ GOALS.md BACKLOG.md Knowledge/\n2. Run: git commit -m \"Update tasks - [current date]\"\n3. Run: git push\n4. Confirm push successful and show commit hash"
  }
}
